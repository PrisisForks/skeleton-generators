{
    "name": "narrowspark/project-configurator",
    "type": "discovery-configurator",
    "description": "A Discovery project configurator for the narrowspark skeleton.",
    "keywords": [
        "narrowspark",
        "discovery",
        "configurator",
        "http",
        "console",
        "full-framework"
    ],
    "homepage": "https://github.com/narrowspark/project-configurator",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "narrowspark/discovery-common": "^0.1.3",
        "nette/php-generator": "^3.1"
    },
    "require-dev": {
        "composer/composer": "^1.6.0",
        "narrowspark/php-cs-fixer-config": "^3.0.1",
        "phpunit/phpunit": "^7.0.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Narrowspark\\Project\\Configurator\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Narrowspark\\Project\\Configurator\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "coverage": "phpunit --coverage-html=\"build/logs\"",
        "cs": "php-cs-fixer fix",
        "phpstan": "phpstan analyse -c phpstan.neon -l 7 src --memory-limit=-1",
        "test": "phpunit"
    },
    "support": {
        "issues": "https://github.com/narrowspark/http-status/issues",
        "source": "https://github.com/narrowspark/http-status"
    }
}

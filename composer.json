{
    "name": "narrowspark/project-configurator",
    "type": "composer-plugin",
    "description": "The project configurator for the narrowspark skeleton is a discovery extension.",
    "keywords": [
        "narrowspark",
        "viserio",
        "discovery",
        "composer-plugin",
        "configurator",
        "http",
        "console",
        "full-framework"
    ],
    "homepage": "https://github.com/narrowspark/project-configurator",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "cakephp/chronos": "^1.2",
        "composer/composer": "^1.6.0",
        "mockery/mockery": "^1.1",
        "narrowspark/discovery-common": "^0.1.6",
        "narrowspark/coding-standard": "^1.0.0",
        "narrowspark/testing-helper": "^6.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "class": "Narrowspark\\Project\\Configurator\\ProjectConfigurator"
    },
    "autoload": {
        "psr-4": {
            "Narrowspark\\Project\\Configurator\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Narrowspark\\Project\\Configurator\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "coverage": "phpunit --coverage-html=\"build/logs\"",
        "cs": "php-cs-fixer fix",
        "phpstan": "phpstan analyse -c phpstan.neon -l 7 src --memory-limit=-1",
        "test": "phpunit"
    },
    "support": {
        "issues": "https://github.com/narrowspark/http-status/issues",
        "source": "https://github.com/narrowspark/http-status"
    }
}
